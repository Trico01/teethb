<!--pages/find/toothcleaner/toothcleaner.wxml-->
<view class="contain">
  <view class="myInput">
    <image class="findImg" src="https://z3.ax1x.com/2021/07/22/W0Pcse.png"></image>
    <input placeholder-class="plas" class="inputs" type="text"></input>
    <view class="divLine"></view>
    <image class="moreImg" mode="aspectFill" src="https://z3.ax1x.com/2021/07/22/W0P7QS.png"></image>
  </view>
</view>
<view class="user">
  <!-- <image src="{{avatarUrl}}"></image>
  <view>{{nickName}}</view> -->
  <view class="white">
    <image src="{{avatarUrl}}" class="userImg"></image>
    <view class="userName">{{nickName}}</view>
    <view class="tip1">关注 39</view>
    <view class="divLine2"></view>
    <view class="tip2">消息提醒 99+</view>
  </view>
</view>
<image class="cleaner" mode="widthFix" src="http://oss.raynor.top/teethb/column1.png"></image>
<swiper  style="height:500rpx" next-margin="70rpx" previous-margin="20rpx">
  <swiper-item>
    <image class="tuijian" src="https://z3.ax1x.com/2021/07/22/W0ajKK.png"></image>
  </swiper-item>
  <swiper-item>
    <image class="tuijian" src="https://z3.ax1x.com/2021/07/22/W0aq81.png"></image>
  </swiper-item>
  <swiper-item>
    <image class="tuijian" src="https://z3.ax1x.com/2021/07/22/W0a759.png"></image>
  </swiper-item>
</swiper>
<image class="cleaner" mode="widthFix" src="http://oss.raynor.top/teethb/column2.png"></image>
<view class="elements">
  <image class="element" mode="widthFix" bindtap="toYashua" src="https://z3.ax1x.com/2021/07/22/W0iGFI.png"></image>
  <image class="element" mode="widthFix" bindtap="toYagao" src="https://z3.ax1x.com/2021/07/22/W0i1wd.png"></image>
  <image class="element" mode="widthFix" bindtap="toBeizi" src="https://z3.ax1x.com/2021/07/22/W0iMOe.png"></image>
</view>